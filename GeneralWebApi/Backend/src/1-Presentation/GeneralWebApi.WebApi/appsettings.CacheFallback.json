{
    "CacheFallback": {
        //false means disable the cache fallback check every 5 minutes
        "EnableDatabaseFallback": false,
        "EnableAutoReconnection": true,
        "HealthCheckIntervalMinutes": 5,
        "ReconnectionIntervalMinutes": 1,
        "MaxReconnectionAttempts": 10,
        "EnableDetailedLogging": true,
        "RefreshTokenKeyPrefix": "refreshToken:",
        "UserInfoKeyPrefix": "user_info:",
        "DefaultCacheExpiryMinutes": 60,
        "RefreshTokenExpiryDays": 7
    },
    "HealthCheck": {
        "EnableHealthCheckEndpoints": true,
        "EnableDetailedHealthInfo": true,
        "HealthCheckTimeoutSeconds": 30,
        "EnableAutoCleanup": true,
        "CleanupIntervalHours": 24,
        "MaxCleanupBatchSize": 1000,
        "EnableCacheRecoveryEndpoints": true,
        "EnableSessionManagementEndpoints": true
    }
}